    <script>
        var max =0;
    function getEvenNumbers() {
        var sum=0;
        var arr=[2,4,5,6,8,9,12,34,51,23,19,35,64,86]

        for (var i = 0; i < arr.length; i++) {
            if (arr[i] % 2 === 0) {
                document.writeln(arr[i] + "even"+ "<br />");
            }

          else if (arr[i] % 2 === 1) {
            document.writeln(arr[i] + "odd"+ "<br />");
          }
                
        }
    }

    getEvenNumbers();




    
    function getEvenNumbers1() {
        var sum=0;
        
        var arr=[2,4,5,6,8,9,12,34,51,23,19,35,64,86]
 document.writeln(Math.min(...arr)  +"the lowest even number" 
 +"<br />");
            
    }

    getEvenNumbers1()


    function getEvenNumbers2() {
        var sum=0;
        var max=0;

        
        var arr=[2,4,5,6,8,9,12,34,51,23,19,35,6,8]
        for(i=0;i<=arr.length;i++){

         if (arr[i] % 2 === 1 && max > arr[i]) {
             max+=arr[i];
           
          }
        }
        document.writeln(Math.max(...arr)  +"the highesr odd number"+"<br />" )

    }

    getEvenNumbers2();


    function getEvenNumbers3() {
        var total=0;
        

        
        var arr=[2,4,5,6,8,9,12,34,51,23,19,35,64,86]
            for(var i=0; i<arr.length-1;i++){
                total += arr[i];
              

            } 
            document.writeln(total +"the total value");     
    }

    getEvenNumbers3();
    </script>
